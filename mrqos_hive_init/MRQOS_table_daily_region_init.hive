USE mrqos;

drop table mrqos_region_day;

create external table mrqos_region_day (
    maprule INT,
    geoname STRING,
    netname STRING,
    region INT,
    avg_region_score FLOAT,
    max_region_score FLOAT,
    avg_region_nsd_demand FLOAT,
    daily_region_nsd_demand INT,
    avg_region_eu_demand FLOAT,
    daily_region_eu_demand INT,
    daily_region_ra_load BIGINT,
    avg_case_percent_pre_norm_load FLOAT,
    avg_case_percent_load FLOAT,
    avg_recorded_load_percentage FLOAT,
    case_percent_load FLOAT,
    case_percent_nsd_demand FLOAT,
    case_percent_eu_demand FLOAT,
    case_ra_load BIGINT,
    case_nsd_demand BIGINT,
    case_eu_demand BIGINT,
    case_sum_max_region_percent_load FLOAT,
    case_uniq_region INT,
    name STRING,
    ecor INT,
    continent STRING,
    country STRING,
    city STRING,
    latitude FLOAT,
    longitude FLOAT,
    asnum INT,
    provider STRING,
    region_capacity BIGINT,
    ecor_capacity BIGINT,
    prp STRING,
    numghosts INT )

partitioned by (datestamp STRING)
row format delimited
fields terminated by '\t'
lines terminated by '\n'
location '/ghostcache/hadoop/data/MRQOS/mrqos_region_day';