SELECT current_timestamp,casename,maprule,geoname,netname,CASE WHEN SUM(bytes) > 0 THEN 100 * SUM(bytes_in) / SUM(bytes) ELSE 0 END in_out_ratio,MAX(targetvalue) target_value,SUM(CAST(100*percentage as integer)) coverage_pct FROM (SELECT maprule,geoname,netname,MIN(targetvalue) targetvalue FROM mrpm_maprule_qos_objectives WHERE attrname = 'IN-OUT-RATIO' GROUP BY 1, 2, 3) a, (SELECT MAX(casename) casename,mapruleid,geo,aslist,regionid,MAX(percentage) percentage FROM a_maprule_qos_view_by_region GROUP BY 2, 3, 4, 5) b, (SELECT region,maprule_id,SUM(bytes) bytes,SUM(bytes_in) bytes_in FROM maprule_info d,mcm_machines_v2 e WHERE ghostip = ip GROUP BY 1, 2) c WHERE a.maprule = b.mapruleid AND a.maprule = c.maprule_id AND b.regionid = c.region AND (a.geoname = b.geo OR a.geoname = 'GLOBAL' AND b.geo = '') AND (a.netname = b.aslist OR a.netname = 'ANY' AND b.aslist = '') GROUP BY 2,3,4,5;
